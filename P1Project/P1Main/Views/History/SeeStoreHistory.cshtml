@model List<ModelsLibrary.StoreOrderDisplay>
@{
  ViewData["Title"] = "SeeStoreHistory";
}

<h1>@ViewBag.StoreLocation order history</h1>

@if (Model.Count == 0)
{
  <h2>No order history available.</h2>
}
else
{
  @foreach (var order in Model)
  {
    <h2>
      <a class="navbar-brand" asp-route-orderId="@order.OrderId" asp-action="SeeOrderDetails">
        Order @order.OrderId made on @order.OrderDate by @order.Username
      </a>
    </h2>
    <h3>Cost: @order.OrderCost</h3>
    <hr />
  }
}

